<Window x:Class="eThanhTra.Resource.TSDWaitWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:eThanhTra.Resource"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        mc:Ignorable="d"
        Title="TSDWaitWindow" Height="450" Width="800" WindowStyle="None" AllowsTransparency="True" Background="Transparent" Topmost="False" ShowInTaskbar="False" WindowState="Maximized" Loaded="Window_Loaded" Closed="Window_Closed">
    <Grid>
        <Image gif:ImageBehavior.AnimatedSource="{StaticResource ImgLoading}" gif:ImageBehavior.AnimationSpeedRatio="5" Stretch="Uniform" Height="100"/>
    </Grid>
</Window>
